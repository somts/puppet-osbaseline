{
  "name": "puppet-osbaseline",
  "version": "0.1.7",
  "author": "somts",
  "summary": "Manage Puppet OS Baseline",
  "license": "BSD-2-Clause",
  "source": "https://github.com/somts/puppet-osbaseline",
  "project_page": "https://github.com/somts/puppet-osbaseline",
  "issues_url": "https://somts.ucsd.edu/support",
  "dependencies": [
    { "name": "puppet-archive" },
    { "name": "puppet-logrotate" },
    { "name": "puppet-openvmtools" },
    { "name": "puppet-selinux" },
    { "name": "puppetlabs-chocolately" },
    { "name": "puppetlabs-concat" },
    { "name": "puppetlabs-git" },
    { "name": "puppetlabs-ntp" },
    { "name": "puppetlabs-powershell" },
    { "name": "puppetlabs-puppet_agent" },
    { "name": "puppetlabs-registry" },
    { "name": "puppetlabs-rsync" },
    { "name": "puppetlabs-stdlib" },
    { "name": "puppet-epel" },
    { "name": "thekevjames-homebrew" },
    { "name": "zleslie-pkgng" }
  ],
  "operatingsystem_support": [
    { "operatingsystem": "CentOS",
      "operatingsystemrelease": ["7", "8"] },
    { "operatingsystem": "Darwin",
      "operatingsystemrelease": ["20", "21"] },
    { "operatingsystem": "Debian",
      "operatingsystemrelease": ["9", "10"] },
    { "operatingsystem": "FreeBSD",
      "operatingsystemrelease": ["12", "13"] },
    { "operatingsystem": "OracleLinux",
      "operatingsystemrelease": ["7", "8"] },
    { "operatingsystem": "RedHat",
      "operatingsystemrelease": ["7", "8"] },
    { "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["16.04", "18.04", "20.04"] },
    { "operatingsystem": "windows",
      "operatingsystemrelease": ["10", "2016", "2019"] }
  ],
  "requirements": [
    { "name": "puppet", "version_requirement": ">= 6.21.0 < 8.0.0" }
  ],
  "pdk-version": "2.4.0",
  "template-url": "https://github.com/puppetlabs/pdk-templates#2.4.0",
  "template-ref": "tags/2.4.0-0-gfa6b6d2"
}
